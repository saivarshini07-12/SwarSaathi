{
  "name": "swarsaathi",
  "version": "1.0.0",
  "description": "SwarSaathi - AI-powered eldercare companion app with voice assistance",
  "homepage": "https://saivarshini07-12.github.io/SwarSaathi",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd murf-backend && npm run dev",
    "start": "npm run dev",
    "build": "cd frontend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd murf-backend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../murf-backend && npm install",
    "predeploy": "npm run build:frontend",
    "deploy": "gh-pages -d frontend/dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "eldercare",
    "voice-assistant",
    "react",
    "nodejs",
    "typescript",
    "sqlite"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "gh-pages": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}